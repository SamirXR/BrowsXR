<html>
  <head>
    <title>Hikari - Results of <%=search_query%></title>
    <link rel="stylesheet" href="/css/app.css">
  </head>
  <body>
    
    <div class="container">
      <div class="search-ui">
        <span class="brand">Hikari Project</span>
        <input type="text" placeholder="what do you want to search?" id="search-input" value="<%=search_query%>">
        <button class="btn" onclick="search();">Search</button>
      </div>
      
      <span class="text-l">Results of: <%=search_query%></span>
      
      <div id="results"></div>
    </div>
    
    <script src="/js/app.js"></script>
    <script>
      let query = "<%=search_query%>";
      initSearch(query);
    </script>
  </body>     
</html>